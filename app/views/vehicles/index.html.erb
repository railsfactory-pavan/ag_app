<p id="notice"><%= notice %></p>

<h1>Vehicles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Color</th>
      <th>Company name</th>
      <th>Parent Categories</th>
      <th>Sub Categories</th>
      <th>Image Galleries</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.name %></td>
        <td><%= vehicle.color %></td>
        <td><%= vehicle.company_name %></td>
        <td>
          <% @vehicle.parent_categories.each do |hello| %>
            <%= hello.parent_category %>
          <% end %>
        </td>
        <td>
          <% @vehicle.sub_categories.each do |hello| %>
            <%= hello.sub_category %>
          <% end %>
        </td>
        <td>
          <% @vehicle.image_galleries.each do |hello| %>
            <%= hello.image %>
          <% end %>
        </td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
